# Honeypot Project Environment Configuration
# Copy this file to .env and update with your values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=honeypot_db
DB_USER=honeypot_user
DB_PASSWORD=change_this_password

# Cowrie Configuration
COWRIE_LOG_PATH=/home/honeypot/cowrie/var/log/cowrie/cowrie.json

# GeoIP Configuration (optional - download from MaxMind)
# https://dev.maxmind.com/geoip/geolite2-free-geolocation-data
GEOIP_DB_PATH=./data/GeoLite2-City.mmdb

# Analysis Settings
BRUTE_FORCE_THRESHOLD=10
LOG_PROCESSING_INTERVAL=60

# Dashboard Settings
DASHBOARD_SECRET_KEY=your-secret-key-here-change-in-production
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PORT=5000

# Security
ENABLE_AUTHENTICATION=False
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin

# Environment (development or production)
FLASK_ENV=development
